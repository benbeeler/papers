@article{harp_scanning_2017,
	title = {Scanning electron microscopy examination of a {Fast} {Flux} {Test} {Facility} irradiated {U}-{10Zr} fuel cross section clad with {HT}-9},
	volume = {494},
	issn = {0022-3115},
	url = {https://www.sciencedirect.com/science/article/pii/S0022311517306529},
	doi = {10.1016/j.jnucmat.2017.07.040},
	abstract = {Observations from a scanning electron microscopy examination of irradiated U-10Zr fuel are presented. The sample studied had a local burnup of 5.7 atom percent and a local inner cladding temperature of 615 °C. This examination by electron microscopy has concentrated on producing data relevant to facilitating a better understanding of Zr redistribution in irradiated U-10Zr fuel and on a better understanding of the complex microstructure present in fuel cladding chemical interaction (FCCI) layers. The presented zirconium redistribution data supplements the existing literature by providing a data set at these particular local conditions. In addition to FCCI layers that are readily visible in optical microscopy, this examination has revealed lanthanide degradation of the cladding by what appears to be a grain boundary facilitated pathway. Precipitates of fission produced Pd-lanthanide compounds were observed in the fuel. Precipitated regions with elevated Mo and elevated W content were also observed in the HT-9 cladding of this sample.},
	urldate = {2024-03-18},
	journal = {Journal of Nuclear Materials},
	author = {Harp, Jason M. and Porter, Douglas L. and Miller, Brandon D. and Trowbridge, Tammy L. and Carmack, William J.},
	month = oct,
	year = {2017},
	keywords = {Fuel cladding chemical interaction (FCCI), Irradiated fuel, Scanning electron microscopy, U-10Zr, Zirconium redistribution},
	pages = {227--239},
	file = {Submitted Version:C\:\\Users\\sheha\\Zotero\\storage\\SQ6FLRKK\\Harp et al. - 2017 - Scanning electron microscopy examination of a Fast.pdf:application/pdf},
}


@techreport{keiser_fuel-cladding_2006,
	title = {Fuel-cladding interaction layers in irradiated {U}-{ZR} and {U}-{PU}-{ZR} fuel elements.},
	url = {https://www.osti.gov/biblio/885496},
	abstract = {Argonne National Laboratory is developing an electrometallurgical treatment for spent nuclear fuels. The initial demonstration of this process is being conducted on U-Zr and U-Pu-Zr alloy fuel elements irradiated in the Experimental Breeder Reactor-II (EBR-II). The electrometallurgical treatment process extracts usable uranium from irradiated fuel elements and places residual fission products, actinides, process Zr, and cladding hulls (small segments of tubing) into two waste forms--a ceramic and a metal alloy. The metal waste form will contain the cladding hulls, Zr, and noble metal fission products, and it will be disposed of in a geologic repository. As a result, the expected composition of the waste form will need to be well understood. This report deals with the condition of the cladding, which will make up a large fraction of the metal waste form, after irradiation in EBR-II and before insertion into the electrorefiner. Specifically, it looks at layers that can be found on the inner surface of the cladding due to in-reactor interactions between the alloy fuel and the stainless steel cladding that occurs after the fuel has swelled and contacted the cladding. Many detailed examinations of fuel elements irradiated in EBR-II have been completed and are discussed in the context of interaction layer formation in irradiated cladding. The composition and thickness of the developed interaction layers are identified, along with the irradiation conditions, cladding type, and axial location on fuel elements where the thickest interaction layers can be expected to develop. It has been found that the largest interaction zones are observed at combined high power and high temperature regions of fuel elements and for fuel elements with U-Pu-Zr alloy fuel and D9 stainless steel cladding. The most prevalent, non-cladding constituent observed in the developed interaction layers are the lanthanide fission products.},
	language = {ENGLISH},
	number = {ANL-NT-240},
	urldate = {2024-03-18},
	institution = {Argonne National Lab. (ANL), Argonne, IL (United States)},
	author = {Keiser, D. D.},
	month = jan,
	year = {2006},
	doi = {10.2172/885496},
	file = {Full Text PDF:C\:\\Users\\sheha\\Zotero\\storage\\7RBXDWNA\\Keiser - 2006 - Fuel-cladding interaction layers in irradiated U-Z.pdf:application/pdf},
}

