@article{cococcioni_linear_2005,
	title = {Linear response approach to the calculation of the effective interaction parameters in the \${\textbackslash}mathrm\{{LDA}\}+{\textbackslash}mathrm\{{U}\}\$ method},
	volume = {71},
	url = {https://link.aps.org/doi/10.1103/PhysRevB.71.035105},
