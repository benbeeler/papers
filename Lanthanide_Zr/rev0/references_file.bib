	urldate = {2023-03-27},
	journal = {Journal of Nuclear Materials},
	author = {Zhou, Mingjin and Fu, Baoqin and Hou, Qing and Wu, Lu and Pan, Rongjian},
	month = aug,
	year = {2022},
	keywords = {Diffusion, Embedded atom method, Multiscale simulation, Point defect, Zirconium},
	pages = {153772},
}

