	keywords = {Ab initio methods, Alloy theory, Disordered state, Monte Carlo, Solid solution, Special quasirandom structures},
	pages = {13--18},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\sheha\\Zotero\\storage\\GEIVIJMK\\van de Walle et al. - 2013 - Efficient stochastic generation of special quasira.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\sheha\\Zotero\\storage\\5NSZXNLV\\S0364591613000540.html:text/html},
