	month = oct,
	year = {2016},
	keywords = {Ion beam irradiation, Irradiation-induced void and bubble, Metallic nuclear fuel, Swelling, Uranium-zirconium alloy},
	pages = {6--11},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\sheha\\Zotero\\storage\\UF6RJIPW\\Ahn et al. - 2016 - Microstructure of α-U and δ-UZr2 phase uranium–zir.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\sheha\\Zotero\\storage\\GAAI962U\\S0925838816311975.html:text/html},
